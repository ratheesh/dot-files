# Weekend days
regexp=(Su|Sa).*
colours=italic red
=======
# Weekdays
regexp=(Mo|Tu|We|Th|Fr)
colours=green
=======
# Year
regexp=\d{4}
colours=magenta
=======
# Month
regexp=[a-zA-Z]{3,}\s
colours=italic cyan
=======
# Current date
regexp=_.?(\s|\d)?_.?\d
colours=blue on_yellow
=======
# ISO week number
regexp=^\s+\d.*
colours=yellow
